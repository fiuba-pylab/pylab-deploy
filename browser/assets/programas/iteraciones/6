numero = int(input('Ingrese nÃºmero entero positivo mayor que 1 a factorizar: '))
while numero > 0 and numero != 1:
    while numero % 2 == 0:
        print(2)
        numero /= 2 
    divide = 3
    while numero != 1: 
        while numero % divide == 0: 
            print(f'{divide}')
            numero /= divide
        divide += 2
    