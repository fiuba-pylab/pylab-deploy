import math

cadena_a = input('Introduce el valor de a: ')
cadena_b = input('Introduce el valor de b: ')
cadena_c = input('Introduce el valor de c: ')

a = float(cadena_a)
b = float(cadena_b)
c = float(cadena_c)

determinante = b ** 2
determinante = determinante - 4 * a * c

raiz_1 = (- b + math.sqrt(determinante)) / (2.0 * a)
raiz_2 = (- b - math.sqrt(determinante)) / (2.0 * a)

print(f'\n Una soluciÃ³n es {raiz_1} otra es {raiz_2}')
