persona1 = (input("Seleccione una persona"))
persona2 = (input("Seleccione una persona"))
persona3 = (input("Seleccione una persona"))
persona4 = (input("Seleccione una persona"))
persona5 = (input("Seleccione una persona"))
persona6 = (input("Seleccione una persona"))

personas = [persona1, persona2, persona3, persona4, persona5, persona6]
adolescentes = ('adolescentes')
jovenes = ('jovenes')
adultos = ('adultos')
ancianos = ('ancianos')

for persona in personas:
    edad = persona[1]
    if edad >= 12 and edad <= 18:
        adolescentes + persona
    if edad > 18 and edad <= 30:
        jovenes + persona
    if edad > 30 and edad <= 60:
        adultos + persona
    if edad > 60:
        ancianos + persona

print(f'{adolescentes}')
print(f'{jovenes}')
print(f'{adultos}')
print(f'{ancianos}')
