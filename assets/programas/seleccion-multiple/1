angulo1 = int(input("Ingrese el primer ángulo: "))
angulo2 = int(input("Ingrese el segundo ángulo: "))
angulo3 =  int(input("Ingrese el tercer ángulo: "))
if angulo1 <= 0 or angulo2 <= 0 or angulo3 <= 0:
    print("Ángulos no válidos")
elif angulo1 + angulo2 + angulo3 != 180:
    print("No es un triángulo")
elif angulo1 < 90 and angulo2 < 90 and angulo3 < 90:
    print("Acutángulo")
elif angulo1 == 90 or angulo2 == 90 or angulo3 == 90:
    print("Rectángulo")
else:
    print("Obtusángulo")